<local:BasePage x:TypeArguments="local:SolvePuzzleViewModel" x:Class="LogiSolver.Desktop.SolvePuzzlePage"
	  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
	  xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
	  xmlns:local="clr-namespace:LogiSolver.Desktop"
	  mc:Ignorable="d" 
	  d:DesignHeight="450" d:DesignWidth="800"
	  Title="SolvePuzzlePage">

	<Grid>
		<Button Style="{StaticResource MainMenuButton}" Width="100" Height="50" VerticalAlignment="Center" HorizontalAlignment="Center"
				Content="Go to Main Menu" Command="{Binding GoToMainMenuPageCommand}" />
	</Grid>
</local:BasePage>
